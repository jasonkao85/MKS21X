public class Sorts {

    /**method matches name
     */
    public static String name() {
	return "06.Kao.Jason";
    }

    /**Selection sort of int array
     *postcondition: elements of the array will be in increasing order
     *@param data the elements to be sorted
     */
    public static int getMin(int[] data) {
	int min = data[0];
	for (int i : data) {
	    if (i < min) min = i;
	}
	return i;
    }

